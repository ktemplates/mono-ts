{
  "name": "root",
  "version": "0.0.0-development",
  "private": true,
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "build": "scripts/runner.sh type=run cmd=build",
    "build:ci": "scripts/runner.sh type=run cmd=build",
    "build:init": "scripts/runner.sh type=run cmd=build on=@internal/*",
    "test": "lerna exec yarn test --ignore '@types/*' --ignore '@internal/*'",
    "test:ci": "jest --ci --runInBand",
    "clean": "lerna run clean && lerna run clean:cli",
    "clean:all": "lerna run clean -- --all",
    "dev:gwebsite": "scripts/runner.sh type=exec cmd=develop on=gwebsite",
    "build:graph": "scripts/runner.sh type=run cmd=build on=@internal/graph",
    "start:graph": "scripts/runner.sh type=exec cmd=start on=@internal/graph",
    "version:local": "scripts/release.sh cmd=version",
    "version:ci": "scripts/release.sh cmd=version CI=true",
    "release:local": "scripts/release.sh cmd=publish",
    "release:ci": "scripts/release.sh cmd=publish CI=true"
  },
  "devDependencies": {
    "@types/jest": "26.0.0",
    "@types/node": "14.0.13",
    "@typescript-eslint/eslint-plugin": "2.34.0",
    "@typescript-eslint/parser": "2.34.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-loader": "4.0.2",
    "eslint-plugin-prettier": "3.1.3",
    "eslint-plugin-react": "7.20.0",
    "jest": "26.0.1",
    "jest-junit": "10.0.0",
    "lerna": "3.22.1",
    "prettier": "2.0.5",
    "ts-jest": "26.1.0",
    "ts-loader": "7.0.5",
    "typescript": "3.9.5",
    "webpack": "4.43.0",
    "webpack-cli": "3.3.11"
  }
}
